{
    "name": "@dolittle/runtime.events.relativity.grpc",
    "description": "Grpc services for fundamentals",
    "version": "3.2.0",
    "homepage": "https://dolittle.io",
    "author": "Dolittle",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dolittle-fundamentals/dotnet.fundamentals.git"
    },
    "bugs": {
        "url": "https://github.com/dolittle-fundamentals/dotnet.fundamentals/issues"
    },
    "keywords": [
        "dolittle",
        "grpc"
    ],
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib",
        "package.json",
        "**/*.d.ts"
    ],
    "main": "lib/index.js",
    "types": "index.d.ts",
    "typings": "index.d.ts",
    "scripts": {
        "ts:cjs": "tsc -p tsconfig.json",
        "ts:clean": "del './*.d.ts' '**/*.d.ts' '!node_modules/**/*' lib && yarn codegen",
        "build": "yarn clean && yarn ts:cjs",
        "clean": "del dist && yarn ts:clean",
        "codegen": "./generate_proxies.sh",
        "copydeclarations": "find . -name '*.d.ts' -not -path './node_modules/*' -not -path './lib/*' | cpio -pdm ./lib",
        "prepublish": "yarn codegen && yarn build && yarn copydeclarations"
    },
    "dependencies": {
        "@types/google-protobuf": "^3.7.1",
        "grpc": "^1.23.2",
        "ts-protoc-gen": "^0.11.0"
    },
    "devDependencies": {
        "@types/node": "12.0.0",
        "del-cli": "^2.0.0",
        "grpc-tools": "^1.8.0",
        "typescript": "^3.4.5"
    }
}